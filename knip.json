{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": ["src/app/**/*.{ts,tsx}", "src/lib/**/*.{ts,tsx}"],
  "project": ["src/**/*.{ts,tsx}", "*.{ts,tsx,mjs,js}"],
  "ignoreExportsUsedInFile": true,
  "ignoreDependencies": [
    "glob",
    "js-yaml",
    "@eslint/compat",
    "@typescript-eslint/eslint-plugin",
    "@typescript-eslint/parser",
    "@vitest/coverage-v8",
    "baseline-browser-mapping",
    "eslint-config-next",
    "tailwindcss"
  ],
  "rules": {
    "exports": "error",
    "files": "error",
    "unlisted": "warn",
    "types": "error",
    "binaries": "off",
    "enumMembers": "warn"
  },
  "typescript": true
}
